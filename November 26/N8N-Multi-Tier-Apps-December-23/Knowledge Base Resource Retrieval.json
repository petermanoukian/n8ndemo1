    {
      "parameters": {
        "jsCode": "// Helper: prefix relative paths with backend base\nfunction withBase(base, path) {\n  if (!path) return '';\n  if (/^https?:\\/\\//i.test(path)) return path;\n  if (path.startsWith('/')) {\n    return base.replace(/\\/+$/, '') + path;\n  }\n  return base.replace(/\\/+$/, '') + '/' + path.replace(/^\\/+/, '');\n}\n\nconst base = $json.LARAVEL_BACKEND || 'http://localhost:8000';\n\n// Step 1: parse the outer wrapper\nconst outer = JSON.parse(items[0].json.result.contents[0].text);\n\n// Step 2: parse the inner text which is the actual entries array\nconst entries = JSON.parse(outer.contents[0].text);\n\nreturn entries.map(entry => {\n  const kbFile = entry.knowledgebasefile ? withBase(base, entry.knowledgebasefile) : '';\n  const kbImg  = entry.knowledgebaseimg ? withBase(base, entry.knowledgebaseimg) : '';\n  const catFile = entry.cat?.categoryfile ? withBase(base, entry.cat.categoryfile) : '';\n\n  return {\n    json: {\n      id: entry.id,\n      name: entry.name,\n      catid: entry.catid,\n      description: entry.des || '',\n      description_html: entry.dess || '',\n      dater: entry.dater,\n      knowledgebasefile: kbFile,\n      knowledgebaseimg: kbImg,\n      category_id: entry.cat?.id || '',\n      category_name: entry.cat?.name || '',\n      category_department: entry.cat?.department || '',\n      categoryfile: catFile,\n    }\n  };\n});\n\n\n"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
