    {
      "parameters": {
        "jsCode": "const raw = $json.rtf_ai_context || \"\";\n\n// Remove \"(Unknown File)\" blocks + binary garbage\nlet cleaned = raw\n    .replace(/RTF DOCUMENT CONTENT\\s*\\(Unknown File\\).*?\\n/i, \"RTF DOCUMENT CONTENT:\\n\")\n    .replace(/[0-9]{10,}/g, \"\") // long garbage numbers\n    .replace(/\\\\[a-zA-Z]+\\d*/g, \"\") // rtf control words\n    .replace(/\\* [0-9a-zA-Z]{10,}/g, \"\") // huge * 000000 blocks\n    .replace(/-{3,}/g, \"\") // separators\n    .replace(/\\n{3,}/g, \"\\n\\n\") // too many new lines\n    .trim();\n\n// Clean download list\nlet dl = ($json.rtf_download_list || \"\")\n    .replace(/\\n{3,}/g, \"\\n\")\n    .trim();\n\nreturn {\n  rtf_ai_context: cleaned,\n  rtf_download_list: dl\n};\n"
      },
