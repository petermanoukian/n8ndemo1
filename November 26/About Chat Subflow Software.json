    {
      "parameters": {
        "jsCode": "const combinedContext = [];\n\nfor (const item of items) {\n\n    // RTF extraction → item.json.data\n    if (item.json.data) {\n        const rtf = item.json.data;\n\n        // lightweight RTF → text\n        let plain = rtf\n            .replace(/\\\\par[d]?/g, \"\\n\")\n            .replace(/\\\\[a-zA-Z]+\\d?/g, \"\")\n            .replace(/[{}]/g, \"\")\n            .trim();\n\n        const truncated = plain.substring(0, 500);\n\n        combinedContext.push(\n            `RTF DOCUMENT CONTENT (${item.json.name || \"Unknown File\"}) (Truncated):\\n${truncated}...`\n        );\n    }\n}\n\nreturn [{\n    json: {\n        rtf_ai_context: combinedContext.join(\"\\n\\n---\\n\\n\")\n    }\n}];\n\n"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        400,
        -48
      ],
      "id": "f02c33ba-0094-462b-ae44-2df61a0be62e",
      "name": "RTF Text Code"
    },
    {
      "parameters": {
        "jsCode": "const downloadLinks = [];\n\nfor (const item of items) {\n    let fileId = null;\n    let fileName = null;\n\n    if (item.json.originalFileID) fileId = item.json.originalFileID;\n    else if (item.json.id) fileId = item.json.id;\n\n    if (item.json.originalFileName) fileName = item.json.originalFileName;\n    else if (item.json.name) fileName = item.json.name;\n\n    if (item.binary && item.binary.data && item.binary.data.fileName) {\n        fileName = item.binary.data.fileName;\n    }\n\n    if (fileId && fileName) {\n        const url = `https://drive.google.com/uc?export=download&id=${fileId}`;\n        downloadLinks.push(`- **${fileName}** → ${url}`);\n    }\n}\n\nreturn [{\n    json: {\n        rtf_download_list: downloadLinks.join(\"\\n\")\n    }\n}];\n\n"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        400,
        112
      ],
      "id": "71bd732e-55f5-4474-aadf-18bf0940e132",
      "name": "RTF Files Code"
    },
    {
      "parameters": {
        "jsCode": "const combinedContext = [];\n\nfor (const item of items) {\n\n    if (item.json.text) {\n        const truncated = item.json.text.substring(0, 500);\n\n        combinedContext.push(\n            `PDF DOCUMENT CONTENT (${item.json.name || \"Unknown File\"}) (Truncated):\\n${truncated}...`\n        );\n    }\n}\n\nreturn [{\n    json: {\n        pdf_ai_context: combinedContext.join(\"\\n\\n---\\n\\n\")\n    }\n}];\n"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
