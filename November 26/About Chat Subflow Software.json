    {
      "parameters": {
        "jsCode": "const downloadLinks = [];\n\nfor (const item of items) {\n    let fileId = null;\n    let fileName = null;\n\n    if (item.json.originalFileID) fileId = item.json.originalFileID;\n    else if (item.json.id) fileId = item.json.id;\n\n    if (item.json.originalFileName) fileName = item.json.originalFileName;\n    else if (item.json.name) fileName = item.json.name;\n\n    if (item.binary && item.binary.data && item.binary.data.fileName) {\n        fileName = item.binary.data.fileName;\n    }\n\n    if (fileId && fileName) {\n        const url = `https://drive.google.com/uc?export=download&id=${fileId}`;\n        downloadLinks.push(`- **${fileName}** → ${url}`);\n    }\n}\n\nreturn [{\n    json: {\n        pdf_download_list: downloadLinks.join(\"\\n\")\n    }\n}];\n\n\n"
      },
